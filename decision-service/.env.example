# 决策服务环境配置示例

# 应用配置
DEBUG=false
HOST=0.0.0.0
PORT=8001

# 数据库配置
DATABASE_URL=postgresql://user:password@localhost:5432/decision_service

# Redis配置
REDIS_URL=redis://localhost:6379/0

# 机器学习配置
MODEL_CACHE_SIZE=100
DECISION_TIMEOUT=30
BATCH_SIZE_LIMIT=100

# 监控配置
PROMETHEUS_PORT=8002
METRICS_ENABLED=true

# 安全配置
CORS_ORIGINS=["*"]
API_KEY=your-api-key-here