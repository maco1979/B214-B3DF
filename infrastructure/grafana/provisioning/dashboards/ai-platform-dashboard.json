{
  "dashboard": {
    "id": null,
    "uid": "ai-platform-dashboard",
    "title": "AI Platform Dashboard",
    "description": "Comprehensive monitoring dashboard for AI platform",
    "tags": ["ai", "platform", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "System Health",
        "type": "stat",
        "datasource": "Prometheus",
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "system_health_status",
            "legendFormat": "Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": []
        }
      },
      {
